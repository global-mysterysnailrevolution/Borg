# HackForge + Agent Harness — API Keys
# Copy to .env and fill in your keys

# Tavily — Web search for LLMs
TAVILY_API_KEY=tvly-dev-XXXXXXXXXXXXXXXXX
TAVILY_MCP_URL=https://mcp.tavily.com/mcp/?tavilyApiKey=YOUR_KEY

# Reka AI — Multimodal (chat, vision, video, speech, research)
REKA_API_KEY=your_reka_api_key_here

# Modulate — Voice/audio moderation
MODULATE_API_KEY=your_modulate_api_key_here

# Yutori — Scouting, browsing, research
YUTORI_API_KEY=your_yutori_api_key_here

# Fastino Labs — TLM entity extraction, classification, Pioneer personalization
FASTINO_API_KEY=your_fastino_api_key_here
PIONEER_API_KEY=your_pioneer_api_key_here

# Neo4j Aura — Knowledge graph
NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# Senso — Knowledge base for tool documentation
SENSO_API_KEY=your_senso_api_key_here

# Airbyte — Data integration pipelines
AIRBYTE_API_KEY=your_airbyte_api_key_here
AIRBYTE_URL=http://localhost:8000/api/v1

# Render — Cloud deployment
RENDER_API_KEY=your_render_api_key_here

# User config (for AuthForge signups)
USER_EMAIL=your_email@example.com
